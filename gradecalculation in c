#include <stdio.h>
int main(){
int num;
printf("Enter marks");
scanf("%d",&num);
printf(" You entered %d", num); // printing outputs

	if(num >= 85){
	printf("Grade A"); // printing outputs
		}
	else if ( num >=70 && num<=84){ // Note the space between else & if
		printf("Grade B");
		}
	else if ( num >=55 && num<=69){
		printf("Grade C");
		}
	else if ( num >=45 && num<=54){
		printf("Grade D");
		}
  else if(num<40){
  printf("Grade F");
  }
return 0;
}
